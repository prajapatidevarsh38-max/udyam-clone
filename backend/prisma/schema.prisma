generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id           Int      @id @default(autoincrement())
  aadhaar      String   @unique
  pan          String?
  name         String?
  email        String?
  mobile       String?
  pin          String?
  city         String?
  state        String?
  createdAt    DateTime @default(now())
}
